{
	"name": "pageres",
	"version": "4.5.1",
	"description": "Capture website screenshots",
	"license": "MIT",
	"repository": "sindresorhus/pageres",
	"main": "dist",
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"test": "xo && nyc ava",
		"release": "np",
		"build": "del-cli dist && tsc",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"page",
		"website",
		"site",
		"web",
		"url",
		"resolution",
		"size",
		"screenshot",
		"screenshots",
		"screengrab",
		"screen",
		"snapshot",
		"shot",
		"responsive",
		"gulpfriendly",
		"phantom",
		"phantomjs",
		"image",
		"svg",
		"render",
		"html",
		"headless",
		"capture",
		"pic",
		"picture",
		"png",
		"jpg",
		"jpeg"
	],
	"dependencies": {
		"array-differ": "^2.0.3",
		"array-uniq": "^2.0.0",
		"del": "^3.0.0",
		"easydate": "^2.0.0",
		"filenamify": "^2.0.0",
		"filenamify-url": "^1.0.0",
		"fs-write-stream-atomic": "^1.0.2",
		"get-res": "^3.0.0",
		"lodash.template": "^4.0.1",
		"log-symbols": "^2.1.0",
		"make-dir": "^2.0.0",
		"mem": "^4.1.0",
		"plur": "^3.0.1",
		"protocolify": "^2.0.0",
		"screenshot-stream": "^4.1.0",
		"unused-filename": "^1.0.0",
		"viewport-list": "^5.0.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.2.0",
		"@types/image-size": "^0.7.0",
		"@types/node": "^10.12.23",
		"@typescript-eslint/eslint-plugin": "^1.3.0",
		"ava": "^1.2.0",
		"cookie": "^0.3.1",
		"coveralls": "^3.0.0",
		"del-cli": "^1.1.0",
		"eslint-config-xo-typescript": "^0.7.0",
		"get-port": "^4.1.0",
		"get-stream": "^4.1.0",
		"image-size": "^0.7.1",
		"nyc": "^13.2.0",
		"path-exists": "^3.0.0",
		"pify": "^4.0.1",
		"png-js": "^0.1.1",
		"sinon": "^7.2.3",
		"ts-node": "^8.0.2",
		"typescript": "^3.3.3",
		"xo": "^0.24.0"
	},
	"typings": "dist",
	"ava": {
		"babel": false,
		"compileEnhancements": false,
		"extensions": [
			"ts",
			"js"
		],
		"require": [
			"ts-node/register"
		]
	},
	"xo": {
		"extends": "xo-typescript",
		"extensions": [
			"ts"
		],
		"rules": {
			"import/named": "off",
			"import/no-unresolved": "off",
			"no-dupe-class-members": "off",
			"async-promise-executor": "off",
			"ava/no-ignored-test-files": "off",
			"no-async-promise-executor": "off"
		}
	},
	"nyc": {
		"extension": [
			".ts"
		]
	}
}
